;; -*- clojure -*-

;; Load Java bindings dynamic library:
(clojure.lang.RT/loadLibrary "polynomial")

;; Import Java namespace:
(import 'polynomial)

;; Instantiate object:
(def p (Polynomial.))
(.setCoeffs p (VectorDouble. [2.0 3.0 5.0 7.0 11.0 -13.0]))

;; Invoke methods:
(prn (.getCoeffs p))
(prn (.evaluate p 2.0))
